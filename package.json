    "keywords": [
        "hls",
        "frame",
        "accurate",
        "timeline",
        "player",
        "video",
        "segmentation",
        "markers"
    ],
    "main": "./dist/omakase-player.cjs.js",
    "module": "./dist/omakase-player.es.js",
    "types": "./dist/index.d.ts",
    "style": "./dist/style.css",
    "scripts": {
        "dev": "vite --host",
        "int": "tsc --noEmit && vite build --debug",
        "prod": "tsc --noEmit && vite build --debug",
