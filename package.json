        "build:prod": "rm -rf dist && tsc --noEmit && vite build --debug"
    },
    "dependencies": {
        "axios": "1.6.7",
        "decimal.js": "10.4.3",
        "eventemitter3": "5.0.0",
        "hls.js": "1.5.15",
        "konva": "8.3.14",
        "node-webvtt": "1.9.4",
        "parse-hls": "^1.0.7",
        "rxjs": "7.8.1",
        "uuid": "9.0.1",
        "yoga-layout": "2.0.1",
        "zod": "3.23.0-canary.20240222T232722"
    },
    "devDependencies": {
        "@types/uuid": "^9.0.8",
        "sass": "^1.59.2",
        "typescript": "^4.9.5",
        "vite": "^4.1.3",
        "vite-plugin-dts": "^2.0.2"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
